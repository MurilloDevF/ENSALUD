{
  "name": "ensalud-presupuestal",
  "private": true,
  "version": "1.0.0",
  "description": "ENSALUD Presupuestal - Electron + Vite Application",
  "main": "dist/main/main.js",
  "scripts": {
    "build:main": "tsc -p tsconfig.main.json",
    "dev:renderer": "vite --config vite.renderer.config.ts",
    "build:renderer": "vite build --config vite.renderer.config.ts",
    "start": "npm run build:main && electron .",
    "start:electron:dev": "cross-env NODE_ENV=development npm run build:main && electron .",
    "dev": "concurrently \"npm:dev:renderer\" \"npm:start:electron:dev\"",
    "build": "npm run build:renderer && npm run build:main",
    "migrate": "echo \"TODO: migrations\"",
    "seed": "echo \"TODO: seed\"",
    "test": "echo \"TODO: tests\""
  },
  "dependencies": {
    "decimal.js": "^10.4.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^30.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  }
}